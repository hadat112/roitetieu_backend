{
  "name": "roinuoc_backend",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "auth": "nodemon -w src/auth.js --exec \"babel-node src/auth.js --presets es2015,stage-0\"",
    "image": "nodemon -w src/image.js --exec \"babel-node src/image.js --presets es2015,stage-0\"",
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hadat112/roinuoc_backend.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hadat112/roinuoc_backend/issues"
  },
  "homepage": "https://github.com/hadat112/roinuoc_backend#readme",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "express": "^4.13.3",
    "express-handlebars": "^6.0.6",
    "http": "^0.0.1-security",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.29.4",
    "mongoose": "^6.6.5",
    "morgan": "^1.8.0",
    "multer": "^1.4.5-lts.1",
    "resource-router-middleware": "^0.6.0",
    "sharp": "^0.32.0",
    "socket.io": "^4.6.1",
    "unorm": "^1.6.0",
    "uuid": "^9.0.0",
    "vntk": "^1.4.4"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.1.1",
    "nodemon": "^1.9.2"
  }
}
